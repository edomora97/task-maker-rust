<<spec>>

<<block>>

<<decl n of type i32>>
<<decl q of type i32>>
<<decl a of type array of i32>>

ioln:
    item n: i32;
    item q: i32;
ioln:
    <<alloc a to size n of i32>>
    for i upto n <i32>:
        <<block>>

        item a[i]: i32;
for i upto q <i32>:
    <<block>>

    <<decl op of type i32>>
    <<decl l1 of type i32>>
    <<decl r1 of type i32>>
    <<decl l2 of type i32>>
    <<decl r2 of type i32>>
    <<decl x2 of type i32>>
    <<decl l3 of type i32>>
    <<decl r3 of type i32>>
    <<decl x3 of type i32>>
    <<decl l4 of type i32>>
    <<decl r4 of type i32>>
    <<decl l5 of type i32>>
    <<decl r5 of type i32>>
    <<decl x5 of type i32>>
    <<decl s of type i32>>

    ioln:
        item op: i32;
        if op == 1:
            item l1: i32;
            item r1: i32;
        if op == 2:
            item l2: i32;
            item r2: i32;
            item x2: i32;
        if op == 3:
            item l3: i32;
            item r3: i32;
            item x3: i32;
        if op == 4:
            item l4: i32;
            item r4: i32;
        if op == 5:
            item l5: i32;
            item r5: i32;
            item x5: i32;
    ioln:
        item s: i32;

